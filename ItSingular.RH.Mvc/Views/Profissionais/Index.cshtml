
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Lista de Profissionais</h1>

<div class="text-center">
    <form method="post" enctype="multipart/form-data" asp-controller="Profissionais" asp-action="ListaProfissionais">
        <div class="form-group">
            <div class="col-md-10">
                <p>Informe o Nome do Usuário::</p>
                <input type="text" name="usuario">
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-10">
                <p>Informe a senha:</p>
                <input type="password" name="senha">
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-10">
                <input type="submit" value="Obter">
            </div>
        </div>
    </form>
</div>
